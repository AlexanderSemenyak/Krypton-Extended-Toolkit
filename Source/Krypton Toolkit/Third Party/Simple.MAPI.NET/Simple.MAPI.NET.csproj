<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  
  
  <PropertyGroup>
    <LangVersion>preview</LangVersion>
    <RootNamespace>Simple.MAPI.NET</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <AnalysisLevel>latest</AnalysisLevel>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Peter van der Woude</Authors>
    <Copyright>Copyright © 2020 Peter van der Woude</Copyright>
    <Description>A .NET wrapper around Simple MAPI - a set of functions and related data structures to add messaging functionality to Windows-based apps.</Description>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <!--Version configuration-->
  <PropertyGroup>
    <AssemblyVersion>1.1.0</AssemblyVersion>
    <FileVersion>1.1.0</FileVersion>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\Bin\$(configuration)\Third Party\Simple.MAPI.NET\Simple.MAPI.NET.xml</DocumentationFile>
    <OutputPath>..\..\Bin\$(configuration)\Third Party\Simple.MAPI.NET\</OutputPath>
  </PropertyGroup>

</Project>
