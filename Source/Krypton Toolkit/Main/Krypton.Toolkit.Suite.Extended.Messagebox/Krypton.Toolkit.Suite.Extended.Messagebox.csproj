<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;netcoreapp3.1;net5.0-windows;net6.0-windows</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net35;net40;net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;net5.0-windows;net6.0-windows</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <OutputType>library</OutputType>
    <RootNamespace>Krypton.Toolkit.Suite.Extended.Messagebox</RootNamespace>
    <AssemblyName>Krypton.Toolkit.Suite.Extended.Messagebox</AssemblyName>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>
  
  <!--Package Data-->
  <PropertyGroup>
    <PackageId>Krypton.Toolkit.Suite.Extended.Messagebox</PackageId>
    <Description></Description>
  </PropertyGroup>
  
  <!--Version Information-->
  <PropertyGroup>
    <AssemblyVersion>6.0.2108</AssemblyVersion>
    <FileVersion>6.0.2108</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  
  <!--Krypton Toolkit Import-->
  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Canary" Version="6.0.2103.6" />
    <!--<PackageReference Include="Krypton.Toolkit" Version="5.550.2103" />-->
  </ItemGroup>
  
  <!--Binary/Documentation Output-->
  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Main\Krypton.Toolkit.Suite.Extended.Messagebox\Krypton.Toolkit.Suite.Extended.Messagebox.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Main\Krypton.Toolkit.Suite.Extended.Messagebox\</OutputPath>
  </PropertyGroup>
</Project>
