<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <LangVersion>preview</LangVersion>
    <OutputType>library</OutputType>
    <RootNamespace>Krypton.Toolkit.Suite.Extended.Tools</RootNamespace>
    <AssemblyName>Krypton.Toolkit.Suite.Extended.Tools</AssemblyName>
    <UseWindowsForms>true</UseWindowsForms>
    <Configurations>Debug;Release;Nightly</Configurations>
  </PropertyGroup>

  <!--Package Data-->
  <PropertyGroup>
    <PackageId>Krypton.Toolkit.Suite.Extended.Tools</PackageId>
    <Description>An extension to the Krypton standard toolkit. This package holds common tools.</Description>
  </PropertyGroup>
  
  <!--Binary/Documentation Output-->
  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Tools\Krypton.Toolkit.Suite.Extended.Tools.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Tools\</OutputPath>
  </PropertyGroup>
  
</Project>
