<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>    
    <RootNamespace>Krypton.Toolkit.Suite.Extended.Utilities</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Utilities\Krypton.Toolkit.Extended.Utilities.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Utilities\</OutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyVersion>5.550.2006</AssemblyVersion>
    <FileVersion>5.550.2006.0</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>5.550.2006</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Canary" Version="6.0.2006.1" />
  </ItemGroup>

</Project>
