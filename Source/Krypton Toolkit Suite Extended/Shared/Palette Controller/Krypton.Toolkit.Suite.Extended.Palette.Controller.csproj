<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net35;net40;net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1</TargetFrameworks>    
    <RootNamespace>Krypton.Toolkit.Extended.Palette.Controller</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Experimental" Version="5.500.2003" />
  </ItemGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Krypton.Toolkit.Suite.Extended.Palette.Controller\Krypton.Toolkit.Extended.Palette.Controller.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Krypton.Toolkit.Suite.Extended.Palette.Controller\</OutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyVersion>6.0.2005</AssemblyVersion>
    <FileVersion>6.0.2005</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>6.0.2005</Version>
  </PropertyGroup>

</Project>
