<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
    <RootNamespace>Krypton.Toolkit.Extended.Palette.Utilities</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <FileVersion>5.550.2009.0</FileVersion>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <PropertyGroup>
    <!--<PackageId>Krypton.Toolkit.Extended.Palette.Utilities.Canary</PackageId>-->
    <PackageId>Krypton.Toolkit.Suite.Extended.Palette.Utilities</PackageId>
    <PackageOutputPath>..\..\..\..\Bin\NuGet</PackageOutputPath>
    <PackagePath></PackagePath>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>5.550.2009</Version>
    <Description>Contains utilities for palette creation.</Description>
    <RepositoryUrl>https://github.com/Krypton-Suite/Extended-Toolkit</RepositoryUrl>
    <PackageLicenseFile></PackageLicenseFile>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Palette.Utilities\Krypton.Toolkit.Suite.Extended.Palette.Utilities.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Palette.Utilities\</OutputPath>
  </PropertyGroup>

</Project>
