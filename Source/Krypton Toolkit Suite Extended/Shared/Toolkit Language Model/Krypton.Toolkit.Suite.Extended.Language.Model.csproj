<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>    
    <RootNamespace>Krypton.Toolkit.Suite.Extended.Language.Model</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <PropertyGroup>
    <!--<PackageId>Krypton.Toolkit.Extended.Language.Model.Canary</PackageId>-->
    <PackageId>Krypton.Toolkit.Suite.Extended.Language.Model</PackageId>
    <PackageOutputPath>..\..\..\..\Bin\NuGet</PackageOutputPath>
    <PackagePath></PackagePath>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>5.550.2008</Version>
    <Description>Contains language options for certain controls.</Description>
    <RepositoryUrl>https://github.com/Krypton-Suite/Extended-Toolkit</RepositoryUrl>
    <PackageLicenseFile></PackageLicenseFile>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Language.Model\Krypton.Toolkit.Extended.Language.Model.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Language.Model\</OutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyVersion>5.550.2008</AssemblyVersion>
    <FileVersion>5.550.2008.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Canary" Version="6.0.2006.3" />
  </ItemGroup>

</Project>
