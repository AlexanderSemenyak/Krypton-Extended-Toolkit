<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;netcoreapp3.1</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1</TargetFrameworks>
    <RootNamespace>Krypton.Toolkit.Extended.Palette.Controls</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>5.500.2005</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Canary" Version="5.500.2005.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Toolkit Core\Krypton.Toolkit.Suite.Extended.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Controls\Colours\BasicPaletteColourUserControlTest.cs" />
  </ItemGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Libraries\Krypton.Toolkit.Suite.Extended.Palette.Controls\Krypton.Toolkit.Suite.Extended.Palette.Controls.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Libraries\Krypton.Toolkit.Suite.Extended.Palette.Controls\</OutputPath>
  </PropertyGroup>

</Project>
