<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net35;net40;net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1</TargetFrameworks>
    <RootNamespace>Krypton.Toolkit.Suite.Extended.Persistence</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Experimental" Version="5.500.2003" />
  </ItemGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Krypton.Toolkit.Suite.Extended.Persistence\Krypton.Toolkit.Extended.Persistence.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Krypton.Toolkit.Suite.Extended.Persistence\</OutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyVersion>5.500.2004</AssemblyVersion>
    <FileVersion>5.500.2004</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>5.500.2004</Version>
  </PropertyGroup>

</Project>
