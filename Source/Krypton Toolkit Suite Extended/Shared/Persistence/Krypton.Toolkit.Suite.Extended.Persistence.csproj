<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <TargetFrameworks>net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;net5.0</TargetFrameworks>
    <RootNamespace>Krypton.Toolkit.Suite.Extended.Persistence</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>Contains core tools.</Description>
    <RepositoryUrl>https://github.com/Krypton-Suite/Extended-Toolkit</RepositoryUrl>
    <PackageLicenseFile></PackageLicenseFile>
  </PropertyGroup>

  <!--Stable package configuration-
  <PropertyGroup>
    <PackageId>Krypton.Toolkit.Suite.Extended.Persistence</PackageId>
    <PackageOutputPath>..\..\..\..\Bin\NuGet\Stable</PackageOutputPath>
    <PackageIconUrl>https://tinyurl.com/KryptonExtendedToolkitIcon</PackageIconUrl>
  </PropertyGroup>-->

  <!--Canary package configuration-->
  <PropertyGroup>
    <PackageId>Krypton.Toolkit.Suite.Extended.Persistence.Canary</PackageId>
    <PackageOutputPath>..\..\..\..\Bin\NuGet\Canary</PackageOutputPath>
    <PackageIconUrl>https://tinyurl.com/KryptonToolkitCanaryIcon</PackageIconUrl>
  </PropertyGroup>

  <!--Version configuration-->
  <PropertyGroup>
    <AssemblyVersion>5.550.2011</AssemblyVersion>
    <FileVersion>5.550.2011</FileVersion>
    <Version>5.550.2011</Version>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Persistence\Krypton.Toolkit.Suite.Extended.Persistence.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Shared\Krypton.Toolkit.Suite.Extended.Persistence\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Canary" Version="6.0.2008" />
  </ItemGroup>

</Project>
