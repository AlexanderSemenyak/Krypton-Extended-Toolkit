<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
    <RootNamespace>Krypton.Toolkit.Suite.Extended.Standard.Controls</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <PropertyGroup>
    <!--<PackageId>Krypton.Toolkit.Extended.Standard.Controls.Canary</PackageId>-->
    <PackageId>Krypton.Toolkit.Suite.Extended.Standard.Controls</PackageId>
    <PackageOutputPath>..\..\..\..\Bin\NuGet</PackageOutputPath>
    <PackagePath></PackagePath>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>5.550.2009</Version>
    <RepositoryUrl>https://github.com/Krypton-Suite/Extended-Toolkit</RepositoryUrl>    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Canary" Version="6.0.2006.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Shared\Toolkit Core\Krypton.Toolkit.Suite.Extended.Core.csproj" />
    <ProjectReference Include="..\..\Shared\Toolkit Language Model\Krypton.Toolkit.Suite.Extended.Language.Model.csproj" />
    <ProjectReference Include="..\Base\Krypton.Toolkit.Suite.Extended.Base.csproj" />
  </ItemGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Libraries\Krypton.Toolkit.Suite.Extended.Standard.Controls\Krypton.Toolkit.Suite.Extended.Standard.Controls.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Libraries\Krypton.Toolkit.Suite.Extended.Standard.Controls\</OutputPath>
  </PropertyGroup>

</Project>
