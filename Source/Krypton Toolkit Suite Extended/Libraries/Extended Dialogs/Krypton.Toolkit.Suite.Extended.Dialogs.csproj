<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <TargetFrameworks>net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net35;net40;net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1</TargetFrameworks>
    <RootNamespace>Krypton.Toolkit.Extended.Dialogs</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>5.500.2005</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Krypton.Toolkit.Experimental" Version="5.500.2003" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Shared\Toolkit Core\Krypton.Toolkit.Suite.Extended.Core.csproj" />
    <ProjectReference Include="..\..\Shared\Toolkit Language Model\Krypton.Toolkit.Suite.Extended.Language.Model.csproj" />
    <ProjectReference Include="..\Colour Controls\Krypton.Toolkit.Suite.Extended.Colour.Controls.csproj" />
    <ProjectReference Include="..\Palette Controls\Krypton.Toolkit.Suite.Extended.Palette.Controls.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\InputBoxResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>InputBoxResources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\InputBoxResources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>InputBoxResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Controls\Colours\" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Krypton.Toolkit.Suite.Extended.Palette.Controls">
      <HintPath>..\Palette Controls\obj\Debug\net45\Krypton.Toolkit.Suite.Extended.Palette.Controls.dll</HintPath>
    </Reference>
    <Reference Include="PresentationFramework" />
  </ItemGroup>

  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Krypton.Toolkit.Extended.Dialogs.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\</OutputPath>
  </PropertyGroup>

</Project>
